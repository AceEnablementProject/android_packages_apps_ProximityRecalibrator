<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="horizontal"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_gravity="center|fill"
	android:gravity="center|left" android:layout_height="match_parent">
	<TableLayout android:id="@+id/tableLayout1"
		android:layout_height="match_parent" android:layout_width="match_parent">
		<TableRow android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/tableRow7">
			<TextView android:id="@+id/textView4" android:layout_width="wrap_content"
				android:layout_height="30dip" android:gravity="center_vertical"
				android:text="@string/title" android:textStyle="bold"></TextView>
		</TableRow>
		<TableRow android:id="@+id/tableRow5" android:layout_width="match_parent"
			android:layout_height="wrap_content">
			<LinearLayout android:id="@+id/gridView1"
				android:layout_width="match_parent" android:layout_height="wrap_content">
				<TextView android:text="@string/lt_label" android:id="@+id/textView1"
					android:layout_width="220dip" android:layout_height="30dip"
					android:gravity="center_vertical"></TextView>
				<TextView android:layout_height="wrap_content"
					android:layout_gravity="center" android:text="@string/lt_value"
					android:id="@+id/lt_value" android:layout_width="80dip"
					android:gravity="right" android:textSize="11pt"></TextView>
			</LinearLayout>
			<ZoomControls android:layout_height="wrap_content" android:layout_width="wrap_content" android:gravity="center" android:id="@+id/ltZoom"></ZoomControls>
		</TableRow>
		<TableRow android:layout_height="wrap_content" android:id="@+id/tableRow1"
			android:layout_width="match_parent">
			<SeekBar android:layout_height="wrap_content" android:id="@+id/lt_slider"
				android:max="255" android:layout_width="315dip"></SeekBar>
		</TableRow>
		<TableRow android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/tableRow9">
		</TableRow>
		<TableRow android:id="@+id/tableRow6" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<LinearLayout android:id="@+id/gridView2"
				android:layout_width="match_parent" android:layout_height="wrap_content">
				<TextView android:text="@string/ht_label" android:id="@+id/textView2"
					android:layout_width="220dip" android:layout_height="30dip"
					android:gravity="center_vertical"></TextView>
				<TextView android:layout_height="wrap_content"
					android:layout_gravity="center" android:text="@string/ht_value"
					android:id="@+id/ht_value" android:layout_width="80dip"
					android:gravity="right" android:textSize="11pt"></TextView>
			</LinearLayout>
			<ZoomControls android:layout_width="wrap_content" android:id="@+id/htZoom" android:gravity="center|center_vertical" android:layout_height="wrap_content"></ZoomControls>
		</TableRow>
		<TableRow android:layout_width="match_parent" android:id="@+id/tableRow2"
			android:layout_height="match_parent">
			<SeekBar android:layout_height="wrap_content"
				android:layout_width="315dip" android:id="@+id/ht_slider"
				android:max="255"></SeekBar>
		</TableRow>
		<TableRow android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/tableRow8">
		</TableRow>
		<TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content"
			android:layout_height="match_parent">
			<TextView android:textSize="10pt" android:id="@+id/textView3"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:text="@string/ork"></TextView>
		</TableRow>
		<TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content"
			android:layout_height="match_parent">
			<LinearLayout android:id="@+id/gridView3"
				android:layout_width="match_parent" android:layout_height="wrap_content">
				<Button android:layout_height="wrap_content"
					android:text="@string/apply" android:id="@+id/applyBut"
					android:layout_width="100dip" android:gravity="center"></Button>
				<TextView android:text="@string/prox_value" android:id="@+id/ps_status"
					android:layout_height="wrap_content" android:layout_gravity="center"
					android:layout_width="200dip" android:gravity="center"
					android:textSize="11pt"></TextView>
			</LinearLayout>
			<Button android:text="@string/buybeer" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/beerbut"></Button>
		</TableRow>
	</TableLayout>
</LinearLayout>
